<% if(typeof errors != 'undefined'){ %>
  <% errors.forEach(function(error) { %>
    <div class="alert warning">
      <span class="closebtn">&times;</span>
      <%= error.msg %>
    </div>
    <% }); %><% } %>
    <% if(success_msg != ''){ %>
      <div class="alert success">
        <span class="closebtn">&times;</span>
        <%= success_msg %>
      </div>
    <% } %>
    <% if(error_msg != ''){ %>
      <div class="alert info">
        <span class="closebtn">&times;</span>
        <%= error_msg %>
      </div>
    <% } %>
    <% if(error != ''){ %>
      <div class="alert">
        <span class="closebtn">&times;</span>
        <%= error %>
      </div>
    <% } %>
